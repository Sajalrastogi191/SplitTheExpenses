      },
      "edgeToEdgeEnabled": true,
      "package": "com.sajal.splittheexpenses",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "39818964-4fb1-4096-a01a-35eecb9a929a"
